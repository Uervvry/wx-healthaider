page{
  background-color: #efefef;
}
.userinfo {
  display: flex;
  flex-direction: column;
  /*flex-flow: wrap;*/
  align-items: center;
  width: 100%;
  /*background-image: url('/../../images/banner.jpg');*/
  background-size: 100% 100%;
  height:400rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.banner-img-box{
    width: 100%;
    display: flex;
    height: 20rpx;
}
.banner-img{
    width: 100%;
    height: 400rpx;
}
.userinfo-space-top {
  width:100%;
  text-align: right;
}
.icon-iconshezhi01{
    margin-right:50rpx;
    color:white;
    font-size: 26rpx;
}
.userinfo-avatar-c {
  flex: 5;
}
.userinfo-avatar-c2 {
  padding: 4rpx;
  margin-top: 90rpx;
  width: 140rpx;
  height: 140rpx;
  background-color: rgba(255,255,255,.8);
  border-radius: 50%;
}

.userinfo-avatar {
  width: 140rpx;
  height: 140rpx;
  margin: 0 auto;
  border-radius: 50%;
  opacity: 1;
  vertical-align: middle;
}

.userinfo-nickname {
  flex: 2;
  color: white;
}

.my-manage-item {
  background-color: white;
  height: 78rpx;
  padding: 0 8px;
  position: relative;
  border-top: 1px solid #dddddd;
}
.my-manage-item-icon-c-share {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  text-align: center;
  display: inline-block;
  color: white;
}
.my-manage-item-icon-c{
  width:100%;
}
.my-manage-item-1,.my-manage-item-3{
    margin-top: 30rpx;
}
.my-manage-item-5,.my-manage-item-2{
   border-bottom: 1px solid #dddddd;
}

.iconfont{
    font-size: 46rpx;
}
.icon-shuimian{
    color:#4e8aee;
}
.check-out{
   color: rgb(114,114,114);
   font-size:30rpx;
   padding:6rpx 12rpx 0 0;
   float: right;
}
.goto-next{
 padding:6rpx 12rpx 0 0;
  float: right;
  color: rgb(114,114,114);
}

.icon-shouhuan{
    color:#fd9438;
}
.icon-qita{
    color:#1bbd2a;
}
.icon-weibiaoti-,.icon-qingchuhuancun,.icon-yinsishuoming,.icon-ruheshiyong,.icon-tianjia{
    color:#1082bf;
    font-size: 70rpx;
}
.my-manage-item-icon-c1 {
  background-color: #7d7dfa;
}

.my-manage-item-icon-c2 {
  background-color: #ec9401;
}

.my-manage-item-icon-c3 {
  background-color: #32b16c;
}

.my-manage-item-icon-c4 {
  background-color: #f00;
}

.my-manage-item-icon-c5 {
  background-color: #61cbfe;
}

.my-manage-item-icon {
  vertical-align: middle;
}

.my-manage-item-text {
  padding-left: 16rpx;
  font-size: 32rpx;
  text-align: left;
  color: #333333;
}
.icon-arrowright{
  font-size: 10rpx;
}
